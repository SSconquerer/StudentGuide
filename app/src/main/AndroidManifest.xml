<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="android.example.android.myapp">
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE">  </uses-permission>
    <uses-permission android:name="android.permission.INTERNET" ></uses-permission>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity android:name=".uploadActivity"></activity>
        <activity android:name=".ecellevents" />
        <activity android:name=".maps" />
        <activity android:name=".notesActivity" />
        <activity
            android:name=".DepartmentListActivity"
            android:label="@string/title_department_list"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".DepartmentDetailActivity"
            android:label="@string/title_department_detail"
            android:parentActivityName=".DepartmentListActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="android.example.android.myapp.DepartmentListActivity" />
        </activity>
        <activity android:name=".downloadsActivity" />
        <activity android:name=".gogreen" />
        <activity android:name=".shaurya" />
        <activity android:name=".raga" />
        <activity android:name=".nrityam" />
        <activity android:name=".sahyog" />
        <activity android:name=".tech" />
        <activity android:name=".ecell" />
        <activity android:name=".cc" />
        <activity android:name=".events2" />
        <activity android:name=".events" />
        <activity android:name=".Welcome" />
        <activity android:name=".Register" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>